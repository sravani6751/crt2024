#include <stdio.h>

int main() {
    // Write C code here
    char n;
    scanf("%c",&n);
    if(n>=65 && n<=90)
    {
        printf("%c is a uppercase alphabet",n);
    }
    if(n>=97&&n<=122)
    {
        printf("%c is a lowercase alphabet",n);
    }
    if(n>=48&&n<=57)
    {
        printf("%c is a digit",n);
    }

    return 0;
}
