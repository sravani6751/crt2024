#include <stdio.h>

int main() {
    int n,rem,sum,temp;
    scanf("%d",&n);
    temp=n;
    sum=0;
    while(n!=0)
    {
        rem=n%10;
        sum=sum*10+rem;
        n=n/10;
    }
    if(sum==temp)
    {
        printf("%d is a palindrome",temp);
    }
    else
    {
        printf("%d is not a palindrome",temp);
    }

    return 0;
}
